@model AddRemoveUserViewBindingModel

<div class="add-user-list">
    <ul>
        @if (Model.UserViews.Any())
        {
            foreach (var user in Model.UserViews)
            {
                <li>
                    <p>@user.Username</p>
                    <a class="material-icons"
                       asp-area="Teamleader"
                       asp-controller="Task"
                       asp-action="GiveTaskToUser"
                       asp-route-taskId="@Model.ExtraId"
                       asp-route-userId="@user.Id">assignment_turned_in</a>
                </li>
            }
        }
    </ul>
</div>